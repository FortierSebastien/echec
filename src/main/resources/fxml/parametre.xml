<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import jeu_echec.vues.composants.MonAnimation?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.*?>

<StackPane xmlns:fx="http://javafx.com/fxml" styleClass="conteneurPrincipal" fx:id="conteneurPrincipal" fx:controller="echec_javafx.vues.VueParametreFX" VBox.vgrow="ALWAYS">



<VBox VBox.vgrow="ALWAYS" fx:id="anchPane">
  
  
    <VBox styleClass="conteneurParametreSons" VBox.vgrow="ALWAYS">
            <HBox styleClass="texteTitres" >
				<Text styleClass="texteTitre" text="%titre"></Text>
			</HBox>
			<Text styleClass="texteSons" text="%musique"></Text>
			
			<Button text="%avec"/>
			<Button text="%sans"/>
			
			 
			
			
    </VBox>
	<HBox styleClass="conteneurBoutons" HBox.hgrow="ALWAYS" >
			<HBox styleClass="conteneurBoutonCancel" HBox.hgrow="ALWAYS">
                <Button fx:id="boutonRetour" text="%retour"/>	
			</HBox>
			<HBox styleClass="conteneurBoutonSave" HBox.hgrow="ALWAYS" >
                <Button fx:id="boutonChangerLangue" text="%changerLangue"/>
            </HBox>
	</HBox>
         
</VBox>



</StackPane>

