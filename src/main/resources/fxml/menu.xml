<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import jeu_echec.vues.composants.MonAnimation?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.*?>

<StackPane xmlns:fx="http://javafx.com/fxml" styleClass="conteneurPrincipal" fx:id="conteneurPrincipal"
 fx:controller="echec_javafx.vues.VueMenuFX" VBox.vgrow="ALWAYS" alignment="center">

<VBox VBox.vgrow="ALWAYS" fx:id="anchPane">
    
    <VBox VBox.vgrow="ALWAYS">
		
	
		<HBox styleClass="titreMain" HBox.hgrow="ALWAYS">
                <Text styleClass="titreMainText" text="%jeu"/>	
			</HBox>
		
		<VBox styleClass="nouvellePartie" VBox.vgrow="ALWAYS">
                <Button fx:id="boutonNouvellePartie" styleClass="button1" text="%nouvellePartie"/>	
			</VBox>
			
			
			<VBox styleClass="nouvellePartie" VBox.vgrow="ALWAYS" >
                <Button fx:id="boutonParametres" styleClass="button2"  text="%titre"/>
            </VBox>
			
			
    </VBox>

</VBox>



</StackPane>

